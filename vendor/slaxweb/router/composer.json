{
    "name": "slaxweb/router",
    "description": "Router component for the SlaxWeb/Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":">=7.0.0",
        "slaxweb/hooks": "dev-master",
        "slaxweb/logger": "dev-master",
        "slaxweb/config": "dev-master",
        "slaxweb/getset": "~0.1",
        "symfony/http-foundation": "~3.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.2",
        "codeception/specify": "*",
        "behat/gherkin": "~4.4",
        "phpunit/phpunit": "~5.4"
    },
    "suggest": {
        "pimple/pimple": "Make use of available providers for Pimple version 3.0.x"
    },
    "autoload": {
        "psr-4": {
            "SlaxWeb\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlaxWeb\\Router\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.x-dev"
        }
    }
}
