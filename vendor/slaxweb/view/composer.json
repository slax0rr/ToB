{
    "name": "slaxweb/view",
    "description": "View library for SlaxWeb Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":  ">=7.0.0",
        "symfony/http-foundation": "~3.0",
        "slaxweb/logger": "dev-master",
        "slaxweb/config": "dev-master"
    },
    "require-dev": {
        "codeception/codeception": "~2.2",
        "codeception/specify": "*",
        "behat/gherkin": "~4.4",
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "SlaxWeb\\View\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlaxWeb\\View\\Test\\Unit\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.x-dev"
        }
    }
}
